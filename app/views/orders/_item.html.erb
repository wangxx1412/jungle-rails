<tr>
  <td>
    <%= image_tag Product.find(item.product_id).image.tiny %>
  </td>
  <td>
    <h4><%= Product.find(item.product_id).name %></h4>
    <br>
    <p><%= Product.find(item.product_id).description %></p>
  </td>
  <td>
    <%= item.quantity %>
  </td>
  <td>
    <%= humanized_money_with_symbol item.total_price_cents / 100.0 %>
  </td>
</tr>